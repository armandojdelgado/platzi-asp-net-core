@model List<Alumno>

@{
    ViewData["Title"] = "Información Alumno";
    Layout = "Simple";//"_Layout";
}

<h1>@ViewData["Title"]</h1>

<table class="table table-dark table-hover">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Id</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var alumno in Model)
        {
            <tr>
                <td>@alumno.Nombre</td>
                <td>@alumno.UniqueId</td>
            </tr>
            
        }
    </tbody>
</table>


<p><i>@ViewBag.CosaDinamica</i></p>
<p><i>@ViewData["CosaDinamica"]</i></p>
<p><i>@ViewData["Fecha"]</i></p>

